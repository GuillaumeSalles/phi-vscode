{
	"name": "phi",
	"displayName": "phi",
	"description": "",
	"version": "0.0.1",
  "enableProposedApi": true,
  "publisher": "Guillaume Salles",
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCustomEditor:testWebviewEditor.catDraw"
	],
	"main": "./out/extension.js",
	"contributes": {
        "customEditors": [
            {
                "viewType": "testWebviewEditor.catDraw",
                "displayName": "Cat Draw",
                "selector": [
                    {
                        "filenamePattern": "*.cat"
                    }
                ]
            }
        ]
    },
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	}
}
